{
  "name": "@needslist/react-filter-url-builder",
  "version": "0.0.2",
  "type": "module",
  "description": "This is a react component that builds a url with filters",
  "main": "./dist/react-filter-url-builder.umd.js",
  "module": "./dist/react-filter-url-builder.es.js",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": "./dist/react-filter-url-builder.es.js",
      "require": "./dist/react-filter-url-builder.umd.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NeedsList/react-filters-url-builder.git"
  },
  "keywords": [
    "react",
    "filters",
    "queryString"
  ],
  "author": "NeedsList",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/NeedsList/react-filters-url-builder/issues"
  },
  "homepage": "https://github.com/NeedsList/react-filters-url-builder#readme",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest run",
    "watch": "vitest"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^9.3.3",
    "@testing-library/react": "^14.1.2",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/react-test-renderer": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.2.0",
    "c8": "^8.0.1",
    "eslint": "^8.54.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "jsdom": "^22.1.0",
    "react-test-renderer": "^18.2.0",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vitest": "^0.34.6"
  }
}
